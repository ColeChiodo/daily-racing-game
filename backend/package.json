{
  "name": "daily-racing-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "npx tsx src/server.ts",
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "migrate": "node-pg-migrate up --migrations-dir ./migrations",
    "migrate:down": "node-pg-migrate down --migrations-dir ./migrations"
